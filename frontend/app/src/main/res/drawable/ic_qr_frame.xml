    <!-- Левый нижний угол -->
    <path
        android:strokeColor="@color/white"
        android:strokeWidth="4"
        android:pathData="M70,250 L50,250 L50,230"/>

    <path
        android:name="animated_corner"
        android:strokeColor="@color/white"
        android:strokeWidth="4"
